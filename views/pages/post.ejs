<!DOCTYPE html>
<html lang="en">
<%- include('../includes/head') %>
<body>
    <%- include("../includes/header")%>
    <main>
        <% if(!posts.length){ %>
            <p>Not posts.</p>
        <%} else {%>
        <ul>
        <% posts.forEach(post =>{ %>
            <li>
                <div style="display:"flex"">
                   <p><%= post.name %> </p>
                   <p><%= post.content %> </p>
                   <span><%= post.time %></span>
                </div> 
            </li>
        <% }) %>
        </ul>
        <% } %>
    </main>
        <%-include('../includes/footer')%>
</body>
</html>